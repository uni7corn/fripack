{
    "base": {
        "version": "1.0.0",
        "fridaVersion": "17.5.1",
        "mode": "embedjs",
        "entry": "main.js",
        "xz": true,
        "outputDir": "./fripack",
        "targetBaseName": "fripack"
    },
    "xp": {
        "inherit": "base",
        "type": "xposed",
        "platform": "android-arm64",
        "xposed": {
            "packageName": "com.fripack.examplexposedmodule",
            "name": "Fripack Xposed Module Example",
        },
        "sign": {
            "keystore": "C:\\Users\\MicroBlock\\.android\\debug.keystore",
            "keystorePass": "android",
            "keystoreAlias": "androiddebugkey"
        },
        "afterBuild": "adb install fripack/fripack-android-arm64.apk",
        "overridePrebuildFile": "E:\\fripack\\build\\fripack-inject\\build\\android\\arm64-v8a\\release\\libfripack-inject.so",
    },
    "xp-arm32": {
        "inherit": "xp",
        "platform": "android-arm32",
    },
    "xp-x86": {
        "inherit": "xp",
        "platform": "android-x86",
    },
    "xp-x86_64": {
        "inherit": "xp",
        "platform": "android-x86_64",
    },
    "android_so": {
        "inherit": "base",
        "type": "shared",
        "platform": "android-arm64",
    },
    "linux_so": {
        "inherit": "base",
        "type": "shared",
        "platform": "linux-x64",
    },
    "windows_dll": {
        "inherit": "base",
        "type": "shared",
        "platform": "windows-x64",
    },
